{
  "name": "tune-guesser",
  "version": "1.0.0",
  "description": "Real-time web-based music guessing game",
  "main": "server/main.py",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && python main.py",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && pip install -r requirements.txt && cd ../client && npm install"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": ["music", "game", "real-time", "deezer", "react"]
}